<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Health_Factory.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Health_Factory"
    xmlns:pages="clr-namespace:Health_Factory.Views"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent Title="Inicio" ContentTemplate="{DataTemplate pages:InicioPage}" Route="InicioPage"/>
    <ShellContent Title="Home" ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage"/>
    <ShellContent Title="Usuario" ContentTemplate="{DataTemplate pages:UsuarioPage}" Route="UsuarioPage"/>
    


</Shell>
